{
  "database": {
    "type": "sqlite",
    "dbname": "./keys.db",
    "max_open_conns": 1,
    "max_idle_conns": 1,
    "conn_max_lifetime": 300
  },
  "processing": {
    "batch_size": 1000
  },
  "key_generation": {
    "total_keys": 1000000,
    "num_workers": 20,
    "min_score": 200,
    "max_letters_count": 5,
    "name": "Your Name",
    "comment": "Your Comment",
    "email": "Your Email"
  },
  "key_encryption": {
    "public_key_path": "path/to/user/public_key.asc"
  },
  "logging": {
    "log_level": "warn",
    "log_file": "gpgenie.log"
  }
}
